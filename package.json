{
  "name": "soybean-admin-vue2",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:prod": "vite --mode production",
    "dev:staging": "vite --mode staging",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "build:dev": "vue-tsc --noEmit --skipLibCheck && vite build --mode development",
    "build:staging": "vue-tsc --noEmit --skipLibCheck && vite build --mode staging",
    "serve": "vite preview",
    "lint": "eslint ./src --ext .vue,.js,jsx,.ts,tsx",
    "lint:fix": "eslint --fix ./src --ext .vue,.js,jsx,.ts,tsx",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@vue/composition-api": "^1.3.0",
    "vue": "^2.6.14",
    "vue-demi": "^0.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/chroma-js": "^2.1.3",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^8.0.0",
    "@vue/runtime-dom": "^3.2.20",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.2",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.43.4",
    "typescript": "^4.4.4",
    "unplugin-icons": "^0.12.16",
    "unplugin-vue-components": "^0.16.0",
    "unplugin-vue2-script-setup": "^0.6.12",
    "vite": "^2.6.13",
    "vite-plugin-html": "^2.1.1",
    "vite-plugin-vue2": "^1.9.0",
    "vite-plugin-windicss": "^1.4.12",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.28.10",
    "windicss": "^3.2.0"
  }
}